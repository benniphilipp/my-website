{
  "dependencies": {
    "bootstrap": "^5.0.1",
    "font-awesome": "^4.7.0",
    "node-sass": "^9.0.0",
    "npm-watch": "^0.11.0"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "compile-sass": "node-sass assets/scss/main.scss static/styles/style.css --watch",
    "watch": "node-sass assets/scss/main.scss static/styles/style.css --watch",
    "cs-watch": "sass assets/scss/main.scss static/styles/style.css --watch --source-map",
    "cs-compile": "sass assets/scss/main.scss static/styles/style.css --source-map",
    "cs-compress": "sass assets/scss/main.scss static/styles/style.css --style=compressed --source-map",
    "cs-prefix": "postcss css/style.css -o static/styles/style.css --use autoprefixer -b 'last 4 versions' --source-map",
    "cs-build": "npm-run-all cs-compile cs-compress cs-prefix",
    "build": "babel assets/js/main.js -o static/js/main.js -f"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.9",
    "@babel/core": "^7.22.9"
  }
}
